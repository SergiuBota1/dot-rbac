{
  "name": "dotkernel/dot-rbac",
  "type": "library",
  "description": "Rbac authorization library for DK",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {

    "php": "^5.6 || ^7.0",
    "psr/http-message": "^1.0",
    "container-interop/container-interop": "^1.1",

    "zendframework/zend-servicemanager": "^3.1",
    "zendframework/zend-stdlib": "^3.1",

    "dotkernel/dot-authorization": "0.6.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "dotkernel/dot-authentication": "0.6.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Rbac\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Rbac\\": "test/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-authorization.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-authentication.git"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
