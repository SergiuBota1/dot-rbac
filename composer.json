{
    "name": "dotkernel/dot-rbac",
    "type": "library",
    "description": "DotKernel RBAC authorization component",
    "license": "MIT",
    "homepage": "https://github.com/dotkernel/dot-navigation",
    "keywords": [
        "laminas",
        "mezzio",
        "rbac"
    ],
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-servicemanager":  "^3.4",
        "dotkernel/dot-authorization": "^3.1",
        "laminas/laminas-dependency-plugin": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "^3.5",
        "laminas/laminas-stdlib": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Rbac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Rbac\\": "test/"
        }
    }
}
